.social{

    &__list{
        width: 15%;
        display: flex;
        justify-content: center;
    }

    &__item{
        width: 31px;
        height: 25px;
        margin-right: 40px;

        &:last-child{
            margin-right: 0;
        }

       

    }

     &__icon{
        width: 100%;
         &:hover{
            opacity: 0.7;
           
        }
    

    }
}